{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "fetch": true
    },
    "rules": {
      "no-console": [ "warn", { "allow": ["error"] } ],
      "react/jsx-filename-extension": [0],
      "comma-dangle": 0,
      "no-underscore-dangle": [ "error", {
        "allow": [
          "_id", "__REDUX_DEVTOOLS_EXTENSION__"
        ] 
      } ],
      "jsx-a11y/no-static-element-interactions": 0
    }
}
